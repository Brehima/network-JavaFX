<StackPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.redhats.network.networkClient.javafx.candidateProfile.SignUp_fxmlController">
   <children>
      <TabPane fx:id="tabs" prefHeight="643.0" prefWidth="858.0">
         <tabs>
            <Tab closable="false" text="Sign IN 1/2">
               <content>
                  <AnchorPane id="AnchorPane" prefHeight="557.0" prefWidth="858.0" style="-fx-background-color: white;">
                     <children>
                        <ImageView fitHeight="68.0" fitWidth="103.0" layoutX="366.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../../../../../../../../../icon/logo.png" />
                           </image>
                        </ImageView>
                        <JFXButton buttonType="RAISED" layoutX="710.0" layoutY="23.0" ripplerFill="RED" text="Sign IN" />
                        <JFXButton buttonType="RAISED" layoutX="74.0" layoutY="23.0" ripplerFill="RED" text="Preview" textFill="#030303" />
                        <HBox layoutX="194.0" layoutY="118.0" prefHeight="354.0" prefWidth="426.0">
                           <children>
                              <VBox prefHeight="354.0" prefWidth="695.0">
                                 <children>
                                    <JFXTextField fx:id="firstName" labelFloat="true" onKeyReleased="#validateFirstName" prefHeight="25.0" prefWidth="366.0" promptText="first name" style="-fx-padding: 20;">
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                    </JFXTextField>
                                    <JFXTextField fx:id="lastName" labelFloat="true" layoutX="10.0" layoutY="10.0" onKeyReleased="#validateLastName" prefHeight="25.0" prefWidth="366.0" promptText="last name" style="-fx-padding: 20;" />
                                    <JFXDatePicker fx:id="birthDate" onAction="#validateDate" overLay="true" prefHeight="25.0" prefWidth="760.0" promptText="your birth date" style="-fx-padding: 20;">
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                    </JFXDatePicker>
                                    <JFXComboBox fx:id="sexe" labelFloat="true" prefHeight="28.0" prefWidth="679.0" promptText="Gender" style="-fx-padding: 20;">
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                    </JFXComboBox>
                                    <JFXTextField fx:id="email" labelFloat="true" layoutX="10.0" layoutY="47.0" prefHeight="25.0" prefWidth="366.0" promptText="email" style="-fx-padding: 20;" />
                                    <JFXTextField fx:id="username" labelFloat="true" layoutX="10.0" layoutY="174.0" prefHeight="25.0" prefWidth="366.0" promptText="username" style="-fx-padding: 20;" />
                                    <JFXPasswordField fx:id="password" labelFloat="true" promptText="password" style="-fx-padding: 20;" />
                                 </children>
                                 <opaqueInsets>
                                    <Insets />
                                 </opaqueInsets>
                                 <effect>
                                    <Lighting>
                                       <bumpInput>
                                          <Shadow />
                                       </bumpInput>
                                       <light>
                                          <Light.Distant />
                                       </light>
                                    </Lighting>
                                 </effect>
                              </VBox>
                           </children>
                        </HBox>
                        <JFXButton layoutX="718.0" layoutY="566.0" onMouseClicked="#goToStep2SignUp" text="next" textAlignment="CENTER" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab closable="false" text="Sign IN 2/2">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" style="-fx-background-color: white;">
                     <children>
                        <ImageView fitHeight="76.0" fitWidth="87.0" layoutX="386.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../../../../../../../../../icon/logo.png" />
                           </image>
                        </ImageView>
                        <JFXButton buttonType="RAISED" layoutX="789.0" layoutY="10.0" prefHeight="19.0" prefWidth="55.0" ripplerFill="RED" text="Sign IN" />
                        <JFXButton buttonType="RAISED" layoutX="-7.0" layoutY="10.0" onMouseClicked="#goToStep1SignUp" ripplerFill="RED" text="Preview" textFill="#030303" />
                        <HBox layoutX="216.0" layoutY="142.0" prefHeight="354.0" prefWidth="426.0">
                           <children>
                              <VBox prefHeight="354.0" prefWidth="695.0">
                                 <opaqueInsets>
                                    <Insets />
                                 </opaqueInsets>
                                 <effect>
                                    <Lighting>
                                       <bumpInput>
                                          <Shadow />
                                       </bumpInput>
                                       <light>
                                          <Light.Distant />
                                       </light>
                                    </Lighting>
                                 </effect>
                                 <children>
                                    <JFXTextArea fx:id="introduction" labelFloat="true" prefHeight="85.0" prefWidth="426.0" promptText="tell us about you in less than 50 words">
