                                          <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                                       </padding></JFXTextArea>
                                    <Pane prefHeight="200.0" prefWidth="200.0">
                                       <children>
                                          <ImageView fx:id="profileImage" fitHeight="201.0" fitWidth="244.0" pickOnBounds="true" preserveRatio="true">
                                             <image>
                                                <Image url="@../../../../../../../../../../../icon/logo.png" />
                                             </image>
                                          </ImageView>
                                          <JFXButton layoutX="245.0" layoutY="174.0" onMouseClicked="#setProfilePicture" text="profile picture" />
                                       </children>
                                       <VBox.margin>
                                          <Insets top="20.0" />
                                       </VBox.margin>
                                    </Pane>
                                 </children>
                              </VBox>
                           </children>
                        </HBox>
                        <JFXButton buttonType="RAISED" layoutX="737.0" layoutY="509.0" onMouseClicked="#finishSignUp" ripplerFill="#006fff" text="skip and finish" />
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
         </tabs>
      </TabPane>
   </children>
</StackPane>
